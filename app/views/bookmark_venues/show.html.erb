<div class="page-header">
  <h3>Bookmark Venue #<%= @bookmark_venue.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Dishes</dt>
      <dd>
        <% if @bookmark_venue.dishes.present? %>
          <a href="/dishes/<%= @bookmark_venue.dishes_id %>">
            <%= @bookmark_venue.dishes.cuisines_id %>
          </a>
        <% end %>
      </dd>

      <dt>User</dt>
      <dd>
        <% if @bookmark_venue.user.present? %>
          <a href="/users/<%= @bookmark_venue.user_id %>">
            <%= @bookmark_venue.user.email %>
          </a>
        <% end %>
      </dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookmark_venues" class="btn btn-primary">
        Back
      </a>
      <a href="/bookmark_venues/<%= @bookmark_venue.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_bookmark_venue/<%= @bookmark_venue.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
