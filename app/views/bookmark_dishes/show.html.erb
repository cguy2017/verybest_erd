<div class="page-header">
  <h3>Bookmark Dish #<%= @bookmark_dish.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Venues</dt>
      <dd><%= @bookmark_dish.venues_id %></dd>

      <dt>User</dt>
      <dd><%= @bookmark_dish.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/bookmark_dishes" class="btn btn-primary">
        Back
      </a>
      <% if current_user == @bookmark_dish.user %>
        <a href="/bookmark_dishes/<%= @bookmark_dish.id %>/edit" class="btn btn-warning">
          Edit
        </a>
        <a href="/delete_bookmark_dish/<%= @bookmark_dish.id %>" class="btn btn-danger" rel="nofollow">
          Delete
        </a>
      <% end %>
    </div>
  </div>
</div>
